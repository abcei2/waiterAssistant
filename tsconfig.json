{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2015",
      "es2016"
    ],
    "jsx": "react-native",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
